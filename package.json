{
  "name": "nuxt-tailwind-template",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "description": "Template for Nuxt.js + Tailwind CSS + DaisyUI",
  "productName": "Nuxt + Tailwind + DaisyUI Template",
  "author": "Joff Tiquez <jofftiquez@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue ./ --fix",
